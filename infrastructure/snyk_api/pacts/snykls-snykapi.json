          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "localCodeEngine": {
            "allowCloudUpload": true,
            "enabled": true,
            "url": "string"
          },
          "org": "string",
          "reportFalsePositivesEnabled": true,
          "sastEnabled": true,
          "supportedLanguages": [
            "string"
