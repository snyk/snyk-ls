{
  "consumer": {
    "name": "SnykLS"
  },
  "provider": {
    "name": "SnykApi"
  },
  "interactions": [
    {
      "description": "happy path without org as query parameter",
      "request": {
        "method": "GET",
        "path": "/cli-config/settings/sast",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "localCodeEngine": {
            "allowCloudUpload": true,
            "enabled": true,
            "url": "string"
          },
          "org": "string",
          "reportFalsePositivesEnabled": true,
          "sastEnabled": true,
          "supportedLanguages": [
            "string"
          ]
        },
        "matchingRules": {
          "$.body.localCodeEngine.allowCloudUpload": {
            "match": "type"
          },
          "$.body.localCodeEngine.enabled": {
            "match": "type"
          },
          "$.body.localCodeEngine.url": {
            "match": "type"
          },
          "$.body.org": {
            "match": "type"
          },
          "$.body.reportFalsePositivesEnabled": {
            "match": "type"
          },
          "$.body.sastEnabled": {
            "match": "type"
          },
          "$.body.supportedLanguages": {
            "min": 1
          },
          "$.body.supportedLanguages[*].*": {
            "match": "type"
          },
          "$.body.supportedLanguages[*]": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}